{
  "rewrites": [
    {
      "source": "/.well-known/xrp-ledger.toml",
      "destination": "https://raw.githubusercontent.com/continentalwick/continental-wick/main/.well-known/xrp-ledger.toml"
    },
    {
      "source": "/.well-known/logo.png",
      "destination": "https://raw.githubusercontent.com/continentalwick/continental-wick/main/.well-known/logo.png"
    }
  ],
  "headers": [
    {
      "source": "/.well-known/xrp-ledger.toml",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=300" }]
    },
    {
      "source": "/.well-known/logo.png",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=86400" }]
    }
  ]
}
